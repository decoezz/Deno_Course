{
  "tasks": {
    "dev": "deno run --watch main.ts"
  },
  "imports": {
    "@oak/oak": "jsr:@oak/oak@^17.1.4",
    "@std/assert": "jsr:@std/assert@1",
    "oak": "https://deno.land/x/oak@v12.5.0/mod.ts"
  },
  "nodeModulesDir": "auto",
  "lint": {
    "rules": {
      "tags": ["recommended"],
      "exclude": ["no-unused-vars", "no-empty"]
    }
  },
  "fmt": {
    "options": {
      "semiColons": true,
      "semi": true
    }
  }
}
